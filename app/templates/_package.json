{
  "name": "<%- props.appName %>",
  "version": "0.0.0",
  "dependencies": {},
  "scripts": {
    "test": "gulp test"
<% if (props.jsPreprocessor.key === 'typescript') { -%>
        ,"postinstall": "tsd install"
<% } -%>
  },
  "devDependencies": {
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "~3.1.0",
    "gulp-angular-architecture-graph": "~0.0.3",
    "gulp-angular-templatecache": "~1.8.0",
    "del": "~2.2.0",
    "eslint": "1.10.3",
    "lodash": "~4.3.0",
    "gulp-minify-css": "~1.2.1",
    "intern": "3.0.6",
    "run-sequence": "1.1.5",
    "gulp-filter": "~3.0.1",
    "gulp-flatten": "~0.2.0",
    "gulp-htmlhint": "0.3.1",
    "gulp-jshint": "2.0.0",
    "jshint-stylish": "2.1.0",
<% if (imageMin) { -%>
    "gulp-imagemin": "~2.3.0",
<% } -%>
    "gulp-eslint": "~1.1.1",
    "eslint-plugin-angular": "~0.15.0",
    "gulp-load-plugins": "~1.2.0",
    "gulp-size": "~2.0.0",
    "gulp-uglify": "~1.5.2",
    "gulp-strip-debug": "^1.1.0",
    "gulp-useref": "~3.0.5",
    "gulp-util": "~3.0.7",
    "gulp-ng-constant": "~1.1.0",
<% if (props.jsPreprocessor.srcExtension !== 'es6' && props.jsPreprocessor.key !== 'typescript') { -%>
    "gulp-ng-annotate": "~1.1.0",
<% } -%>
    "gulp-ngdocs": "https://github.com/groupe-sii/gulp-ngdocs/tarball/master",
    "gulp-replace": "~0.5.4",
    "gulp-rename": "~1.2.2",
    "gulp-rev": "~7.0.0",
    "gulp-rev-replace": "~0.4.3",
    "gulp-htmlmin": "~1.3.0",
    "gulp-inject": "~3.0.0",
    "gulp-sourcemaps": "~1.6.0",
<% if (props.cssPreprocessor.key === 'node-sass') { -%>
    "gulp-sass": "~2.2.0",
    "gulp-scss-lint": "0.3.9",
<% } if (props.cssPreprocessor.key === 'ruby-sass') { -%>
    "gulp-ruby-sass": "~0.7.1",
    "gulp-scss-lint": "0.3.9",
<% } if (props.cssPreprocessor.key === 'less') { -%>
    "gulp-less": "~3.0.3",
<% } if (props.cssPreprocessor.key === 'stylus') { -%>
    "gulp-stylus": "~2.0.6",
<% } if (props.jsPreprocessor.srcExtension === 'es6' || props.jsPreprocessor.key === 'typescript') { -%>
    "webpack-stream": "~2.1.1",
    "ng-annotate-loader": "0.0.10",
<% } if (props.jsPreprocessor.srcExtension === 'es6') { -%>
    "eslint-loader": "~1.1.0",
<% } if (props.jsPreprocessor.key === 'babel') { -%>
    "babel-core": "~5.8.25",
    "babel-loader": "~5.3.2",
<% } if (props.jsPreprocessor.key === 'traceur') { -%>
    "traceur-loader": "~0.6.3",
<% } if (props.jsPreprocessor.key === 'typescript') { -%>
    "typescript": "~1.6.2",
    "ts-loader": "~0.8.0",
    "tslint-loader": "~1.0.2",
    "tsd": "~0.6.5",
<% } else if (props.jsPreprocessor.srcExtension !== 'es6') { -%>
    "gulp-angular-filesort": "~1.1.1",
<% } if (props.htmlPreprocessor.key !== 'noHtmlPrepro') { -%>
    "gulp-consolidate": "~0.1.2",
<% } if (props.htmlPreprocessor.key === 'jade') { -%>
    "jade": "~1.11.0",
<% } if (props.htmlPreprocessor.key === 'haml') { -%>
    "haml": "~0.4.3",
<% } if (props.htmlPreprocessor.key === 'handlebars') { -%>
    "handlebars": "~4.0.2",
<% } -%>
    "main-bower-files": "~2.11.1",
    "wiredep": "~3.0.0",
    "plato": "1.5.0",
    "browser-sync": "~2.11.1",
    "browser-sync-spa": "~1.0.3",
    "http-proxy-middleware": "~0.10.0",
    "chalk": "~1.1.1",
<% if (qrCode) { -%>
    "qrcode-terminal": "~0.10.0",
<% } -%>
    "uglify-save-license": "~0.4.1",
    "wrench": "~1.5.8",
    "minimist": "^1.2.0",
    "gulp-tar": "1.8.0",
    "gulp-gzip": "1.2.0",
    "gulp-run": "1.6.12"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
