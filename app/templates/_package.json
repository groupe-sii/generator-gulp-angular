{
  "name": "<%- props.appName %>",
  "version": "0.0.0",
  "dependencies": {},
  "scripts": {
    "test": "gulp test"
  },
  "devDependencies": {
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~2.3.1",
    "gulp-angular-architecture-graph": "~0.0.3",
    "gulp-angular-templatecache": "~1.7.0",
    "del": "~1.2.0",
    "eslint": "0.24.0",
    "eslint-plugin-angular": "0.0.15",
    "lodash": "~3.10.0",
    "gulp-csso": "~1.0.0",
    "intern": "2.2.2",
    "run-sequence": "1.1.1",
    "gulp-eslint": "0.15.0",
    "gulp-filter": "~2.0.2",
    "gulp-flatten": "~0.1.0",
    "gulp-htmlhint": "0.2.0",
<% if (imageMin) { -%>
    "gulp-imagemin": "~2.2.0",
<% } -%>
    "gulp-jshint": "~1.11.2",
    "gulp-load-plugins": "~1.0.0-rc.1",
    "gulp-size": "~1.2.3",
    "gulp-uglify": "~1.2.0",
    "gulp-useref": "~1.2.0",
    "gulp-util": "~3.0.6",
    "gulp-ng-annotate": "~1.0.0",
    "gulp-ngdocs": "https://github.com/groupe-sii/gulp-ngdocs/tarball/master",
    "gulp-replace": "~0.5.3",
    "gulp-rename": "~1.2.2",
    "gulp-rev": "~5.1.0",
    "gulp-rev-replace": "~0.4.2",
    "gulp-run": "1.6.8",
    "gulp-minify-html": "~1.0.3",
    "gulp-inject": "~1.3.1",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-ng-constant": "0.3.0",
<% if (props.cssPreprocessor.key === 'node-sass') { -%>
    "gulp-sass": "~2.0.3",
    "gulp-scss-lint": "^0.2.1",
<% } if (props.cssPreprocessor.key === 'ruby-sass') { -%>
    "gulp-ruby-sass": "~0.7.1",
<% } if (props.cssPreprocessor.key === 'less') { -%>
    "gulp-less": "~3.0.3",
<% } if (props.cssPreprocessor.key === 'stylus') { -%>
    "gulp-stylus": "~2.0.5",
<% } if (props.jsPreprocessor.key === 'coffee') { -%>
    "gulp-coffee": "~2.3.1",
    "gulp-coffeelint": "~0.5.0",
<% } if (props.jsPreprocessor.srcExtension === 'es6') { -%>
    "webpack-stream": "~2.0.0",
    "jshint-loader": "~0.8.3",
<% } if (props.jsPreprocessor.key === 'babel') { -%>
    "babel-core": "~5.5.8",
    "babel-loader": "~5.1.4",
<% } if (props.jsPreprocessor.key === 'traceur') { -%>
    "traceur-loader": "~0.6.3",
<% } if (props.jsPreprocessor.key === 'typescript') { -%>
    "gulp-typescript": "~2.7.7",
    "gulp-tslint": "~2.0.0",
    "gulp-concat": "~2.5.2",
    "tsd": "~0.6.1",
<% } else if (props.jsPreprocessor.srcExtension !== 'es6') { -%>
    "gulp-angular-filesort": "~1.1.1",
<% } if (props.htmlPreprocessor.key !== 'none') { -%>
    "gulp-consolidate": "~0.1.2",
<% } if (props.htmlPreprocessor.key === 'jade') { -%>
    "jade": "~1.11.0",
<% } if (props.htmlPreprocessor.key === 'haml') { -%>
    "haml": "~0.4.3",
<% } if (props.htmlPreprocessor.key === 'handlebars') { -%>
    "handlebars": "~3.0.3",
<% } -%>
    "main-bower-files": "~2.8.2",
    "merge-stream": "~0.1.8",
    "jshint-stylish": "~2.0.1",
    "wiredep": "~2.2.2",
    "concat-stream": "~1.5.0",
    "require-dir": "~0.3.0",
    "minimist": "^1.1.1",
    "plato": "1.5.0",
    "browser-sync": "~2.7.13",
    "browser-sync-spa": "~1.0.2",
    "http-proxy-middleware": "~0.1.0",
    "chalk": "~1.1.0",
<% if (qrCode) { -%>
    "qrcode-terminal": "~0.10.0",
<% } -%>
    "uglify-save-license": "~0.4.1",
    "wrench": "~1.5.8"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
